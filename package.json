{
  "name": "website-boilerplate",
  "private": true,
  "description": "A simple website boilerplate.",
  "version": "0.1.0",
  "author": "Darryl Snow <darryl@yourweb.expert>",
  "dependencies": {
    "@types/react-helmet": "^5.0.15",
    "gatsby": "^5.0.1",
    "gatsby-image": "^2.2.34",
    "gatsby-plugin-manifest": "^2.2.31",
    "gatsby-plugin-offline": "^3.0.27",
    "gatsby-plugin-react-helmet": "^3.1.16",
    "gatsby-plugin-sharp": "^2.3.5",
    "gatsby-source-filesystem": "^2.1.40",
    "gatsby-transformer-sharp": "^2.3.7",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "gatsby-plugin-typescript": "^2.1.23",
    "prettier": "^1.19.1",
    "stylelint": "^12.0.1",
    "stylelint-order": "^4.0.0",
    "typescript": "^3.7.4"
  },
  "keywords": [
    "website"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx && stylelint ./src/**/*.css"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/darryl-snow/website-boilerplate"
  },
  "bugs": {
    "url": "https://github.com/darryl-snow/website-boilerplate/issues"
  }
}
